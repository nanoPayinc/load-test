{
  "node": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "eqeqeq": true,
  "eqnull": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "nonew": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "trailing": true,
  "sub": true,
  "maxlen": 120,
  "strict": true,
  "curly": true,
  "forin": false,
  "maxparams": 4,
  "maxdepth": 2,
  "maxcomplexity": 5,
  "validthis": true,
  "globals": {
    "AuthorizationError": false,
    "ValidationError": false,
    "PaymentError": false
  }
}
